{
  "rules": {
    "react/react-in-jsx-scope": "off", // Evite d'ajouter "import React from 'react'" (surtout dans la v17)
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }], // Permet de faire du JSX dans du js
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  },
  "extends": ["airbnb", "airbnb/hooks", "prettier"]
}